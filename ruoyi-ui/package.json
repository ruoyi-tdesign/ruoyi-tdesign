{
  "private": "true",
  "name": "@tencent/tdesign-vue-next-starter",
  "version": "0.8.0",
  "description": "Ruoyi-Tdesign多租户管理系统",
  "scripts": {
    "dev:mock": "vite --open --mode mock",
    "dev": "vite --open --mode development",
    "dev:linux": "vite --mode development",
    "build:test": "vite build --mode test --minify false",
    "build": "vite build --mode release",
    "build:lint": "vue-tsc --noEmit && vite build --mode release",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less}",
    "site:preview": "npm run build && cp -r dist _site",
    "test": "echo \"no test specified,work in process\"",
    "test:coverage": "echo \"no test:coverage specified,work in process\""
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^5.1.1",
    "axios": "^1.5.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "echarts": "5.1.2",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "prismjs": "^1.29.0",
    "qrcode.vue": "^3.4.1",
    "qs": "^6.11.2",
    "tdesign-icons-vue-next": "0.2.2",
    "tdesign-vue-next": "1.5.6",
    "tinymce": "^6.7.0",
    "tvision-color": "^1.6.0",
    "vue": "^3.3.4",
    "vue-clipboard3": "^2.0.0",
    "vue-cropper": "^1.1.1",
    "vue-router": "~4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/crypto-js": "^4.1.2",
    "@types/echarts": "^4.9.18",
    "@types/js-cookie": "^3.0.4",
    "@types/lodash": "^4.14.198",
    "@types/nprogress": "^0.2.0",
    "@types/prismjs": "^1.26.0",
    "@types/qs": "^6.9.8",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vue-scoped-css": "^2.5.0",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.0",
    "stylelint": "~15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "~6.0.3",
    "typescript": "~5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-prismjs": "^0.0.8",
    "vite-plugin-vue-devtools": "^0.5.1",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix"
    ],
    "*.{html,vue,css,sass,less}": [
      "npm run stylelint:fix"
    ]
  }
}
