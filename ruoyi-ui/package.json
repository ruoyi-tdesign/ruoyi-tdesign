{
  "private": "true",
  "name": "tdesign-vue-next-starter",
  "version": "0.7.4",
  "description": "Ruoyi-Tdesign多租户管理系统",
  "scripts": {
    "dev:mock": "vite --open --mode mock",
    "dev": "vite --open --mode development",
    "dev:linux": "vite --mode development",
    "build:test": "vite build --mode test --minify false",
    "build": "vite build --mode release",
    "build:lint": "vue-tsc --noEmit && vite build --mode release",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,vss,sass,less}",
    "site:preview": "npm run build && cp -r dist _site",
    "test": "echo \"no test specified,work in process\"",
    "test:coverage": "echo \"no test:coverage specified,work in process\""
  },
  "dependencies": {
    "ckeditor5-overall-build": "^0.1.2",
    "@ckeditor/ckeditor5-vue": "^5.1.0",
    "@highlightjs/vue-plugin": "^2.1.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "echarts": "5.1.2",
    "file-saver": "2.0.5",
    "fuse.js": "6.6.2",
    "highlight.js": "^11.8.0",
    "js-cookie": "^3.0.5",
    "jsencrypt": "3.3.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qrcode.vue": "^3.3.4",
    "qs": "^6.11.1",
    "tdesign-icons-vue-next": "^0.1.11",
    "tdesign-vue-next": "^1.3.5",
    "tvision-color": "^1.6.0",
    "vue": "^3.2.47",
    "vue-clipboard3": "^2.0.0",
    "vue-cropper": "^1.0.8",
    "vue-router": "~4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/echarts": "^4.9.16",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.191",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/compiler-sfc": "^3.3.2",
    "@vue/eslint-config-typescript": "^11.0.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "eslint-plugin-vue-scoped-css": "^2.4.0",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.7",
    "stylelint": "~14.9.1",
    "stylelint-config-prettier": "~9.0.5",
    "stylelint-less": "1.0.6",
    "stylelint-order": "~6.0.3",
    "typescript": "~4.9.5",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-vue-devtools": "^0.0.11",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.7.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix",
      "git add ."
    ],
    "*.{html,vue,vss,sass,less}": [
      "npm run stylelint:fix",
      "git add ."
    ]
  }
}
